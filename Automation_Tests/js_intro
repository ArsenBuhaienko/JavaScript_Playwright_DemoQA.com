let list = [12, "woof", true, 4.22, [2,6,5], {"Blyat":12}]

//=============================================================================================================================================================

function map_iterator(map){
    for (var [key, value] of map) {
        console.log(key + " have " + value + " strength")
      }
}


  function array_iterator1(list){
    for(let f=0; f<list.length; f++){
        console.log("Item = " + list[f])
      }
  }
  

  function array_iterator2(list){
    for(let item in list){
        console.log("Item = " + list[item])
    }
  }


function find_array_in_json(users){
    for(let item in users){

        console.log(users[item])
    
        if(typeof users[item] == 'object'){
            for(let obj_item in users[item]){
                if(typeof users[item][obj_item] == 'object'){
                    for(let arr_item of users[item][obj_item]){
                        console.log("Array ==", arr_item)
                    }
                    
                }
            }
        }else{
            console.log("NOT OBJ")
        }
      }
}

//=============================================================================================================================================================

